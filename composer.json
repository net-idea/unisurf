{
  "name": "unisurf/website",
  "description": "Website and management system for UniSurf - Unique Surfing Dormagen",
  "authors": [
    {
      "name": "Adam Ibrom",
      "email": "adam@net-i.de"
    }
  ],
  "homepage": "https://unisurf.de/",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.4.1",
    "doctrine/doctrine-bundle": "^3.2.2",
    "doctrine/doctrine-migrations-bundle": "^4.0",
    "doctrine/orm": "^3.6.1",
    "phpdocumentor/reflection-docblock": "^6.0.0",
    "phpstan/phpdoc-parser": "^2.3.1",
    "symfony/asset": "^8.0",
    "symfony/asset-mapper": "^8.0.3",
    "symfony/console": "^8.0.3",
    "symfony/doctrine-messenger": "^8.0.3",
    "symfony/dotenv": "^8.0",
    "symfony/expression-language": "^8.0",
    "symfony/flex": "^2.10",
    "symfony/form": "^8.0.3",
    "symfony/framework-bundle": "^8.0.3",
    "symfony/http-client": "^8.0.3",
    "symfony/intl": "^8.0.1",
    "symfony/lock": "^8.0.3",
    "symfony/mailer": "^8.0.3",
    "symfony/mime": "^8.0",
    "symfony/monolog-bundle": "^4.0.1",
    "symfony/notifier": "^8.0",
    "symfony/process": "^8.0.3",
    "symfony/property-access": "^8.0.3",
    "symfony/property-info": "^8.0.3",
    "symfony/rate-limiter": "^8.0",
    "symfony/runtime": "^8.0.1",
    "symfony/security-bundle": "^8.0",
    "symfony/serializer": "^8.0.3",
    "symfony/stimulus-bundle": "^2.32",
    "symfony/string": "^8.0.1",
    "symfony/translation": "^8.0.3",
    "symfony/twig-bundle": "^8.0.3",
    "symfony/ux-turbo": "^2.32",
    "symfony/validator": "^8.0.3",
    "symfony/web-link": "^8.0",
    "symfony/webpack-encore-bundle": "^2.4",
    "symfony/yaml": "^8.0.1",
    "twig/extra-bundle": "^2.12|^3.22.2",
    "twig/twig": "^3.22.2",
    "erusev/parsedown": "^1.7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "dev-master",
    "phpstan/phpstan": "^2.1.33",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/phpstan-doctrine": "^2.0.12",
    "phpstan/phpstan-strict-rules": "^2.0.7",
    "phpstan/phpstan-symfony": "^2.0.9",
    "phpunit/phpunit": "^12.5.4",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "^8.0.3",
    "symfony/css-selector": "^8.0",
    "symfony/debug-bundle": "^8.0",
    "symfony/maker-bundle": "^1.65.1",
    "symfony/stopwatch": "^8.0",
    "symfony/web-profiler-bundle": "^8.0.3"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "plesk/composer-command-info": false
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  }
}
